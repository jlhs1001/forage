{
  "name": "forage",
	"displayName": "forage",
	"description": "forage search tool. let no information be hidden.",
	"publisher": "LiamSeewald",
	"repository": {"url": "https://github.com/jlhs1001/forage.git"},
	"license": "LICENSE",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.64.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:forage.find"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "forage.find",
				"title": "Hello World"
			}
		],
		"keybindings": [
			{
				"command": "forage.find",
				"key": "ctrl+alt+j",
				"mac": "alt+cmd+j"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.64.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}
